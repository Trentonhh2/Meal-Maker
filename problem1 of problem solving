var numbers = [1, 3, 5, 8];

var products = [];

for (let i = 0; i < numbers.length; i++) {
  var currentNum = numbers[i];
  var nextNum = numbers[i + 1];
  
  var product = currentNum * nextNum;

  if (product) {
    products.push(product);
  }
};

var max = Math.max(...products);

console.log(max);
